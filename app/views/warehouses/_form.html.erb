<%= form_with model: warehouse do |f| %>
  <div>
    <%= f.label :name, 'Nome' %>
    <%= f.text_field :name %>
    <% warehouse.errors.full_messages_for(:name).each do |msg| %>
        <div><%= msg %></div>
    <% end %>
  </div>

  <div>
    <%= f.label :description, 'Descrição' %>
    <%= f.text_area :description %>
    <% warehouse.errors.full_messages_for(:description).each do |msg| %>
        <div><%= msg %></div>
    <% end %>
  </div>

  <div>
    <%= f.label :code, 'Código' %>
    <%= f.text_field :code %>
    <% warehouse.errors.full_messages_for(:code).each do |msg| %>
        <div><%= msg %></div>
    <% end %>
  </div>

  <div>
    <%= f.label :city, 'Cidade' %>
    <%= f.text_field :city %>
    <% warehouse.errors.full_messages_for(:city).each do |msg| %>
        <div><%= msg %></div>
    <% end %>
  </div>

  <div>
    <%= f.label :area, 'Área' %>
    <%= f.number_field :area %>
    <% warehouse.errors.full_messages_for(:area).each do |msg| %>
        <div><%= msg %></div>
    <% end %>
  </div>

  <div>
    <%= f.label :address, 'Endereço' %>
    <%= f.text_field :address %>
    <% warehouse.errors.full_messages_for(:address).each do |msg| %>
        <div><%= msg %></div>
    <% end %>
  </div>

  <div>
    <%= f.label :cep, 'CEP' %>
    <%= f.text_field :cep %>
    <% warehouse.errors.full_messages_for(:cep).each do |msg| %>
        <div><%= msg %></div>
    <% end %>
  </div>

  <div>
    <%= f.submit 'Enviar' %>
  </div>
<% end %>
